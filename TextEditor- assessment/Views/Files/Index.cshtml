@model IEnumerable<TextEditor__assessment.Models.file>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                File Name
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in @ViewBag.fileNames) {
        <tr>
            <td>
                @item.fileName
            </td>
            <td>
                @Html.ActionLink("Open", "View", new {  id=item.id  }) |
                @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
}
    </tbody>
</table>
